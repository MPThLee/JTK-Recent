{
  "name": "jtk-recent",
  "version": "1.0.0",
  "description": "A Visited Site generator with hacky way.",
  "license": "AGPL-3.0",
  "private": true,
  "engines": {
    "node": ">= 16.1 <17"
  },
  "devDependencies": {
    "@commitlint/cli": "~13.1.0",
    "@commitlint/config-conventional": "~13.1.0",
    "@prettier/plugin-pug": "~1.16.5",
    "@types/luxon": "~2.0.1",
    "@types/minimist": "~1.2.2",
    "@types/node": "~16.7.3",
    "@types/pug": "~2.0.5",
    "@types/qs": "~6.9.7",
    "@typescript-eslint/eslint-plugin": "~4.29.3",
    "@typescript-eslint/parser": "~4.29.3",
    "eslint": "~7.32.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-prettier": "~3.4.1",
    "husky": "~7.0.2",
    "prettier": "~2.3.2",
    "rimraf": "~3.0.2",
    "surge": "~0.23.0",
    "tsutils": "~3.21.0",
    "typescript": "~4.3.5"
  },
  "scripts": {
    "start": "node build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "start:gen": "node build/src/list.js",
    "deploy": "surge --project ./dist --domain jtk-test-env.surge.sh",
    "aio": "npm run start && npm run start:gen && npm run deploy",
    "aio:build": "npm run build && npm run aio",
    "aio:test_build": "npm run build && npm run start && npm run start:gen",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "~0.21.1",
    "dotenv": "~10.0.0",
    "fs-extra": "~10.0.0",
    "full-icu": "~1.3.4",
    "luxon": "~2.0.2",
    "minimist": "~1.2.5",
    "pug": "~3.0.2",
    "qs": "~6.10.1",
    "tslib": "~2.3.1"
  },
  "volta": {
    "node": "16.2.0",
    "npm": "7.15.0",
    "pnpm": "6.4.0"
  }
}