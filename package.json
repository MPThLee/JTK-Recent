{
  "name": "jtk-recent",
  "version": "1.0.0",
  "description": "A Visited Site generator with hacky way.",
  "license": "AGPL-3.0",
  "private": true,
  "engines": {
    "node": ">= 16.1 <17"
  },
  "devDependencies": {
    "@commitlint/cli": "~18.6.1",
    "@commitlint/config-conventional": "~18.6.2",
    "@prettier/plugin-pug": "~2.5.2",
    "@types/luxon": "~3.4.2",
    "@types/minimist": "~1.2.5",
    "@types/node": "~20.14.12",
    "@types/pug": "~2.0.10",
    "@types/qs": "~6.9.17",
    "@typescript-eslint/eslint-plugin": "~5.62.0",
    "@typescript-eslint/parser": "~5.62.0",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "~9.1.0",
    "eslint-plugin-prettier": "~4.2.1",
    "husky": "~9.1.7",
    "prettier": "~2.8.8",
    "rimraf": "~5.0.8",
    "surge": "~0.24.6",
    "tsutils": "~3.21.0",
    "typescript": "~5.6.3"
  },
  "scripts": {
    "start": "node build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "start:gen": "node build/src/list.js",
    "deploy": "surge --project ./dist --domain jtk-test-env.surge.sh",
    "aio": "npm run start && npm run start:gen && npm run deploy",
    "aio:build": "npm run build && npm run aio",
    "aio:test_build": "npm run build && npm run start && npm run start:gen",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "~0.29.0",
    "dotenv": "~16.4.5",
    "fs-extra": "~11.2.0",
    "full-icu": "~1.5.0",
    "luxon": "~3.5.0",
    "minimist": "~1.2.8",
    "pug": "~3.0.3",
    "qs": "~6.13.1",
    "tslib": "~2.8.1"
  },
  "volta": {
    "node": "16.15.0",
    "npm": "8.8.0",
    "pnpm": "7.0.0"
  }
}
