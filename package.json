{
  "name": "jtk-recent",
  "version": "1.0.0",
  "description": "A Visited Site generator with hacky way.",
  "license": "AGPL-3.0",
  "private": true,
  "engines": {
    "node": ">= 16.1 <17"
  },
  "devDependencies": {
    "@commitlint/cli": "~17.6.3",
    "@commitlint/config-conventional": "~17.6.3",
    "@prettier/plugin-pug": "~2.5.0",
    "@types/luxon": "~3.3.0",
    "@types/minimist": "~1.2.2",
    "@types/node": "~18.16.3",
    "@types/pug": "~2.0.6",
    "@types/qs": "~6.9.7",
    "@typescript-eslint/eslint-plugin": "~5.59.6",
    "@typescript-eslint/parser": "~5.59.6",
    "eslint": "~8.40.0",
    "eslint-config-prettier": "~8.8.0",
    "eslint-plugin-prettier": "~4.2.1",
    "husky": "~8.0.3",
    "prettier": "~2.8.8",
    "rimraf": "~5.0.0",
    "surge": "~0.23.1",
    "tsutils": "~3.21.0",
    "typescript": "~5.0.4"
  },
  "scripts": {
    "start": "node build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "start:gen": "node build/src/list.js",
    "deploy": "surge --project ./dist --domain jtk-test-env.surge.sh",
    "aio": "npm run start && npm run start:gen && npm run deploy",
    "aio:build": "npm run build && npm run aio",
    "aio:test_build": "npm run build && npm run start && npm run start:gen",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "~0.27.2",
    "dotenv": "~16.0.3",
    "fs-extra": "~11.1.1",
    "full-icu": "~1.5.0",
    "luxon": "~3.3.0",
    "minimist": "~1.2.8",
    "pug": "~3.0.2",
    "qs": "~6.11.2",
    "tslib": "~2.5.0"
  },
  "volta": {
    "node": "16.15.0",
    "npm": "8.8.0",
    "pnpm": "7.0.0"
  }
}
